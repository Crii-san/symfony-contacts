{% extends 'base.html.twig' %}

{% block title %}Liste des contacts{% endblock %}

{% block body %}
<h1>Liste des contacts</h1>
    <ul>
    {% for user in contacts %}
        <li>
            <a href="{{ path('detail_contact', {id: user.id}) }}">
                {{ user.lastname }} {{ user.firstname }}</a>
        </li>
    {% endfor %}
    </ul>
{% endblock %}
