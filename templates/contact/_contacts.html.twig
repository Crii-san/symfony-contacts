{% block body %}
    <h1>Liste des contacts</h1>
    <ul class="contacts">
        {% for user in contacts %}
            <li>
                <a href="{{ path('detail_contact', {id: user.id}) }}">
                    <span class="lastname"> {{ user.lastname }}</span> <span class="firstname">{{ user.firstname }}</span></a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}